{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "rules": {
        "max-len": [1, 170, 2, {"ignoreComments": true}],
        "react/prop-types": [0],
        "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
        "no-shadow": ["off"],
        "no-param-reassign": "off",
        "no-unused-vars": "off",
        "react/react-in-jsx-scope": "off",
        "no-undef": "off"
    }
}